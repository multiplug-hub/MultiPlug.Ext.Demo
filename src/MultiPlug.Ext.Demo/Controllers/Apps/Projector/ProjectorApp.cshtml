    <script>
        var YellowArray = new Array();
        YellowArray[0] = new Array('T<span style="color: red">h</span>e', '316');
        YellowArray[1] = new Array('con<span style="color: red">s</span>umables', '171');
        YellowArray[2] = new Array('a<span style="color: red">r</span>e', '350');
        YellowArray[3] = new Array('ru<span style="color: red">n</span>ning', '275');
        YellowArray[4] = new Array('l<span style="color: red">o</span>w', '375');
        YellowArray[5] = new Array('<span style="color: red">o</span>n', '411');
        YellowArray[6] = new Array('ma<span style="color: red">c</span>hine', '201');
        YellowArray[7] = new Array('<span style="color: red">2</span>', '400');
        YellowArray[8] = new Array('l<span style="color: red">i</span>ne', '397');
        YellowArray[9] = new Array('<span style="color: red">4</span>', '408');

        var RedArray = new Array();
        RedArray[0] = new Array('L<span style="color: red">i</span>ne', '351');
        RedArray[1] = new Array('<span style="color: red">4</span>', '408');
        RedArray[2] = new Array('h<span style="color: red">a</span>s', '327');
        RedArray[3] = new Array('st<span style="color: red">o</span>pped.', '294');
        RedArray[4] = new Array('', '375');
        RedArray[5] = new Array('Ma<span style="color: red">c</span>hine', '201');
        RedArray[6] = new Array('<span style="color: red">2</span>', '409');
        RedArray[7] = new Array('h<span style="color: red">a</span>s', '327');
        RedArray[8] = new Array('r<span style="color: red">u</span>n', '361');
        RedArray[9] = new Array('o<span style="color: red">u</span>t', '328');
        RedArray[10] = new Array('o<span style="color: red">f</span>', '347');
        RedArray[11] = new Array('con<span style="color: red">s</span>umables.', '171');


        var speedReadTimer;
        var speedReadDiv;
        var speedReadIndex;
        var speedReadArray;

        function startYellowSpeedRead()
        {
            clearTimeout(speedReadTimer);
            speedReadDiv = $('#YellowSpeedRead');
            speedReadIndex = -1;
            speedReadArray = YellowArray;
            mycode();
        }

        function startRedSpeedRead()
        {
            clearTimeout(speedReadTimer);
            speedReadDiv = $('#RedSpeedRead');
            speedReadIndex = -1;
            speedReadArray = RedArray;
            mycode();
        }

        function stopSpeedRead()
        {
            clearTimeout(speedReadTimer);
        }

        function mycode() {

            if (speedReadIndex == -1)
            {
                speedReadDiv.html("");
                speedReadIndex = 0;
                speedReadTimer = setTimeout(mycode, 2000);
                return;
            }

            speedReadDiv.html(speedReadArray[speedReadIndex][0]);
            speedReadDiv.css("padding-left", speedReadArray[speedReadIndex][1] + "px");

            speedReadIndex++;

            if (speedReadIndex == speedReadArray.length)
            {
                speedReadIndex = -1;
            }

            speedReadTimer = setTimeout(mycode, 350);
        }

        //var state = 0;

        function SetState(state)
        {
            var GreenState = $('#greenstate');
            var YellowState = $('#yellowstate');
            var RedState = $('#redstate');

            switch(state) {
                case 1:
                    GreenState.css("visibility", "hidden");
                    RedState.css("visibility", "hidden");
                    YellowState.css("visibility", "visible");
                    StartTime();
                    startYellowSpeedRead();
                    break;
                case 2:
                    YellowState.css("visibility", "hidden");
                    GreenState.css("visibility", "hidden");
                    RedState.css("visibility", "visible");
                    StartTime();
                    startRedSpeedRead();

                    break;
                default:
                    StopTime();
                    stopSpeedRead();
                    RedState.css("visibility", "hidden");
                    YellowState.css("visibility", "hidden");
                    GreenState.css("visibility", "visible");
            } 

            //if (state == 2)
            //{
            //    state = 0;
            //}
            //else
            //{
            //    state++;
            //}

            //setTimeout(CycleStates, 10000);
        }

        //CycleStates();

        SetState(0);

        var minutesLabel = $(".minutes");
        var secondsLabel = $(".seconds");
        var totalSeconds = 0;

        var secondsTimer;

        function StartTime()
        {
            secondsLabel.html("00");
            minutesLabel.html("00");
            clearTimeout(secondsTimer);
            totalSeconds = 0;
            secondsTimer = setInterval(tickTime, 1000);
        }

        function StopTime()
        {
            clearTimeout(secondsTimer);
        }

        function tickTime() {
            ++totalSeconds;
            secondsLabel.html(pad(totalSeconds % 60));
            minutesLabel.html(pad(parseInt(totalSeconds / 60)));
        }

        function pad(val) {
            var valString = val + "";
            if (valString.length < 2) {
                return "0" + valString;
            } else {
                return valString;
            }
        }

        window.addEventListener("multiplugReady", function (e) {
            $.connection.wS.on("Send", function (id, Group) {

                if (id == 'greentimeout') {
                    SetState(1);
                }
                else if (id == 'yellowtimeout') {
                    SetState(2);
                }
                else if (id == 'redtimeout') {
                    SetState(0);
                }
                else if (id == '2583dac7-0fc5-4c78-92fd-b2b86e2ca98c')
                {
                    var UrlTV = Group.Pairs.find(function (pair) { return pair.Type == 'url' });

                    document.getElementById("qrcodeimageyellow").src = UrlTV.Value;
                    document.getElementById("qrcodeimagered").src = UrlTV.Value;
                }
            });
        });

    </script>
</body>
