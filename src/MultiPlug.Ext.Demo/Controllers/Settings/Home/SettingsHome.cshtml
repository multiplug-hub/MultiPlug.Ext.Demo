@model MultiPlug.Base.Http.EdgeApp

<section class="row-fluid">
    <div class="row-fluid">
        <div class="box">
            <div class="span12">
                <p style="font-size:26px; line-height: 54px; text-align: center; margin: 0px;">Demo and Debug</p>
            </div>
        </div>
    </div>

    @Include("MultiPlug.Ext.Demo.Demo_Settings_Navigation")

    <div class="row-fluid">
        <div class="span12">
            <h3 class="box-header">About</h3>
            <div class="box">
                <div class="row-fluid">
                    <div class="span6"><b>Version</b></div>
                    <div class="span6">@Raw(Model.Extension.Model.Version)</div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>Extension</b></div>
                    <div class="span6">@Raw(Model.Extension.Model.Title)</div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>Description</b></div>
                    <div class="span6">@Raw(Model.Extension.Model.Description)</div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>Maintainer</b></div>
                    <div class="span6"><a target="_blank" href="https://github.com/multiplug-hub/">@Raw(Model.Extension.Model.Company)</a></div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>Product</b></div>
                    <div class="span6">@Raw(Model.Extension.Model.Product)</div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>Copyright</b></div>
                    <div class="span6">@Raw(Model.Extension.Model.Copyright)</div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>Trademarks</b></div>
                    <div class="span6">@Raw(Model.Extension.Model.Trademark)</div>
                </div>
                <div class="row-fluid">
                    <div class="span6"><b>More information</b></div>
                    <div class="span6"><a target="_blank" href="https://github.com/multiplug-hub/MultiPlug.Ext.Demo/">github.com/multiplug-hub/MultiPlug.Ext.Demo/</a></div>
                </div>
            </div>
        </div>
    </div>
</section>
